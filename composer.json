        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.5.* || 5.6.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ronnel Martinez",
            "email": "misc.woe@outlook.ph",
